!
vrf instance INTERNET
ip routing vrf INTERNET
!
interface Vxlan1
   vxlan vrf INTERNET vni 50005
!   
router bgp {{switch_bgp_asn}}
   vrf INTERNET
      rd {{rd_switch_loopback0}}:50005
      route-target import evpn 50005:50005
      route-target export evpn 50005:50005
      update wait-install 
!
